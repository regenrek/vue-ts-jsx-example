{
  "name": "@nujek/ui",
  "version": "0.0.95",
  "description": "Nujek framework",
  "repository": "git@gitlab.com:nujek2/nujek.git",
  "author": "Kevin Regenrek <hello@regenrek.at>",
  "license": "MIT",
  "scripts": {
    "build": "node ./scripts/build",
    "release:gh": "npm test && bump --commit --tag --push --preid alpha && conventional-github-releaser -p angular",
    "release:local": "npm test && npm run readme:update && bump --commit --tag --preid alpha && npm run publich:ci",
    "publish:ci": "node ./scripts/publish.js all",
    "publish": "echo `use publish:ci`",
    "import": "node ./scripts/import",
    "typings": "tsc --p packages --emitDeclarationOnly && node ./scripts/typing-fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint './{packages,.storybook,scripts}/**/*.{js,ts,tsx,vue,md}'",
    "lint:fix": "npm run lint -- --fix",
    "readme:update": "node ./scripts/updateReadme",
    "clean": "rimraf dist typings storybook-static",
    "dev": "start-storybook -p 6006",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "npm run typings && build-storybook",
    "serve": "vue-cli-service serve",
    "build-vue": "vue-cli-service build",
    "lint-vue": "vue-cli-service lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{packages,.storybook,scripts}/**/*.{js,ts,tsx,vue,md} *.js": [
      "eslint --fix",
      "git add"
    ]
  },
